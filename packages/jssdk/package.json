{
  "name": "@bitrix24/b24jssdk",
  "version": "0.2.3",
  "author": "Bitrix",
  "license": "MIT",
  "description": "Bitrix24 REST API JS SDK",
  "keywords": [
    "bitrix24",
    "js",
    "restapi",
    "REST",
    "API"
  ],
  "homepage": "https://bitrix24.github.io/b24jssdk/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitrix24/b24jssdk.git",
    "directory": "packages/jssdk"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": {
        "require": "./dist/commonjs/index.d.cts",
        "import": "./dist/esm/index.d.mts",
        "default": "./dist/esm/index.d.ts"
      },
      "default": {
        "require": "./dist/commonjs/index.cjs",
        "import": "./dist/esm/index.mjs",
        "default": "./dist/esm/index.mjs"
      }
    }
  },
  "main": "./dist/commonjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/esm/index.d.ts",
  "unpkg": "dist/umd/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "dist",
    "package.json"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepack": "npm run build",
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "qs-esm": "^7.0.2",
    "luxon": "^3.6.1",
    "@types/luxon": "^3.6.2"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "eslint-config-unjs": "^0.4.2",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vue-tsc": "^2.2.8"
  },
  "resolutions": {}
}
