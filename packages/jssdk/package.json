{
  "name": "@bitrix24/b24jssdk",
  "version": "0.4.0",
  "author": "Bitrix",
  "license": "MIT",
  "description": "Bitrix24 REST API JS SDK",
  "keywords": [
    "bitrix24",
    "js",
    "nodejs",
    "restapi",
    "REST",
    "API"
  ],
  "homepage": "https://bitrix24.github.io/b24jssdk/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitrix24/b24jssdk.git",
    "directory": "packages/jssdk"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/esm/index.d.mts",
      "import": "./dist/esm/index.mjs"
    }
  },
  "main": "./dist/esm/index.mjs",
  "module": "./dist/esm/index.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/esm/index.d.mts"
      ]
    }
  },
  "unpkg": "dist/umd/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "dist",
    "package.json"
  ],
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepack": "npm run build",
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "tsx ./test/esm/uuid.test.ts"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "qs-esm": "^7.0.2",
    "luxon": "^3.6.1",
    "@types/luxon": "^3.6.2"
  },
  "devDependencies": {
    "@types/node": "^22.15.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "eslint-config-unjs": "^0.4.2",
    "tsx": "^4.19.4"
  },
  "resolutions": {}
}
